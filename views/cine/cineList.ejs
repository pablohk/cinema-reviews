  <div class="container">
    <div class="row">
      <a href="/user/home"><span class="glyphicon glyphicon-arrow-left"></span> Return</span></a>
      <ul class="media-list">
      <div class="col-sm-10 col-sm-offset-1" id="logout">
            <% if(typeof(items) !== 'undefined'){
                items.forEach(e =>{%>
              <li class="media">
                <div class="media-body">
                  <div class="well well-lg">
                    <a class="pull-right" href="/cine/<%=e._id%>/detail" title="View Cine Detail">
                      <img class="media-object img-circle" id="img-list" src="/img/claqueta.jpg" alt="profile">
                    </a>
                    <h4 class="media-heading text-uppercase reviews"><%=e.name%> </h4>
                    <ul class="media-date text-uppercase reviews list-inline">
                      <li class="dd"><%=e.phone%></li>
                    </ul>
                    <p class="media-comment"><%=e.address.street%></p>
                    <a class="btn btn-info btn-circle text-uppercase" href="/cine/<%=e._id%>/detail" id="reply"><span class="glyphicon glyphicon-share-alt"></span>Detail</a>
                    <a class="btn btn-warning btn-circle text-uppercase" href="/review/listByCine/<%=e._id%>"><span class="glyphicon glyphicon-comment"></span>View Reviews</a>
                  </div>
                </div>
              </li>
              <%});%>
            <%}else{%>
              <% if ((message) !== "undefined") { %>
              <div class="error-message"><%= message%></div>
              <% }
            }%>
            </ul>
      </div>
    </div>
  </div>
